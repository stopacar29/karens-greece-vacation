{
  "name": "karens-greece-vacation",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "npm run dev --prefix web-app",
    "start": "node node_modules/expo/bin/cli start",
    "start:tunnel": "node node_modules/expo/bin/cli start --tunnel",
    "try": "npm install && node node_modules/expo/bin/cli start",
    "android": "node node_modules/expo/bin/cli start --android",
    "ios": "node node_modules/expo/bin/cli start --ios",
    "web": "node node_modules/expo/bin/cli start --web",
    "build:web": "npm run build --prefix web-app",
    "start:server": "npm start --prefix server",
    "serve": "npm run build:web && npm run start:server"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.0.0",
    "expo": "~54.0.0",
    "expo-constants": "~17.0.0",
    "expo-document-picker": "~13.0.0",
    "expo-file-system": "~18.0.0",
    "expo-linking": "~7.0.0",
    "expo-router": "~5.0.0",
    "expo-status-bar": "~2.0.0",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "~4.4.0",
    "webidl-conversions": "5.0.0"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.0",
    "@babel/core": "^7.25.2",
    "@types/react": "^19.1.0",
    "babel-preset-expo": "~13.0.0",
    "typescript": "~5.3.0"
  },
  "private": true,
  "overrides": {
    "whatwg-url-without-unicode": {
      "webidl-conversions": "5.0.0"
    }
  }
}
